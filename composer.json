{
    "name": "antriver/laravel-site-scaffolding",
    "description": "A bunch of stuff to reduce duplication between Laravel apps.",
    "type": "library",
    "authors": [
        {
            "name": "Anthony Kuske",
            "homepage": "https://anthonykuske.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Antriver\\LaravelSiteScaffolding\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Antriver\\LaravelSiteScaffoldingTests\\": "tests/",
            "Antriver\\LaravelSiteScaffoldingTestApp\\": "test-laravel-app/app/"
        }
    },
    "require": {
        "php": ">=7.3",
        "ext-json": "*",
        "antriver/laravel-database-session-auth": "^2.0",
        "antriver/laravel-model-presenters": "^1.0",
        "antriver/laravel-simple-message-trait": "^5.8",
        "aws/aws-php-sns-message-validator": "^1.6",
        "erusev/parsedown": "^1.7",
        "firebase/php-jwt": "^5.0",
        "google/recaptcha": "^1.2",
        "intervention/image": "^2.5",
        "laravel/framework": "5.8.*",
        "laravel/socialite": "^4.3",
        "league/flysystem-aws-s3-v3": "^1.0",
        "tmd/laravel-password-updater": "^0.0.2",
        "tmd/laravel-registers": "^2.1",
        "tmd/laravel-repositories": "^2.2",
        "tokenly/token-generator": "^1.0",
        "ext-imagick": "*",
        "ext-openssl": "*"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9",
        "phpunit/phpunit": "^7.0",
        "barryvdh/laravel-ide-helper": "^2.6"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Antriver\\LaravelSiteScaffolding\\Providers\\LaravelSiteScaffoldingServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
